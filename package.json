{
  "name": "tfrs",
  "description": "tfrs",
  "version": "0.0.0",
  "private": true,
  "author": {
    "name": "Forrest Desjardins",
    "email": "desjardinsfg@gmail.com",
    "url": "github.com/fdesjardins"
  },
  "dependencies": {
    "superagent": "^5.1.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.5",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "standard": "^13.1.0"
  },
  "engines": {
    "node": ">=7"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "rules": {
      "object-curly-spacing": [
        2,
        "always"
      ],
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "func-style": [
        2,
        "expression"
      ],
      "prefer-arrow-callback": [
        2,
        {
          "allowNamedFunctions": false
        }
      ]
    }
  },
  "files": [
    "index.js"
  ],
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": "FAA-Aviation-Data-Portal/packages",
  "scripts": {
    "test": "standard *.js && nyc mocha --opts mocha.opts test.js"
  }
}
